{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "APOLLO_API_KEY": "@apollo_api_key",
    "CALENDLY_API_KEY": "@calendly_api_key",
    "AUTOMATION_LEVEL": "98_percent",
    "DAILY_REVENUE_TARGET": "1000",
    "ENVIRONMENT": "production"
  }
}